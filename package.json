{
  "version": "0.0.1",
  "description": "contracts typescript API",
  "name": "contracts-bootstrap",
  "directories": {
    "test": "test"
  },
  "files": [
    "contracts/interface/",
    "abi/",
    "es/typescript/typechain",
    "Address.json",
    "tsconfig.json"
  ],
  "types": "./es/typescript/typechain/index.d.ts",
  "module": "./es/typescript/typechain/index.js",
  "scripts": {
    "build": "npx hardhat compile",
    "prebuild": "rm -rf abi/*",
    "postbuild": "node build.js && npm run typechain",
    "typechain": "rm -rf es && rm -rf typescript/typechain/* && typechain --target ethers-v5 --out-dir typescript/typechain/ './abi/*.json'",
    "posttypechain": "tsc && cp typescript/typechain/*.d.ts es/typescript/typechain"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.5",
    "@nomiclabs/hardhat-etherscan": "^3.0.1",
    "@nomiclabs/hardhat-waffle": "^2.0.2",
    "@openzeppelin/contracts": "^4.5.0",
    "@openzeppelin/test-environment": "^0.1.9",
    "@openzeppelin/test-helpers": "^0.5.15",
    "@truffle/hdwallet-provider": "^2.0.10",
    "@typechain/ethers-v5": "^7.0.1",
    "@types/mocha": "^9.1.0",
    "chai": "^4.3.4",
    "chai-bignumber": "^3.0.0",
    "dotenv": "^10.0.0",
    "erc721a": "^2.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.2.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.4",
    "hardhat": "^2.8.2",
    "hardhat-gas-reporter": "^1.0.6",
    "prettier": "^2.5.1",
    "prettier-plugin-solidity": "^1.0.0-beta.13",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.17",
    "truffle": "^5.5.21",
    "typechain": "^5.1.2",
    "typescript": "^4.4.3",
    "web3": "^1.7.0",
    "lodash": "^4.17.21"
  },
  "author": "MyFrogs",
  "license": "ISC",
  "dependencies": {
    "web3": "^1.7.0",
    "ethers": "^5.5.4"
  }
}
